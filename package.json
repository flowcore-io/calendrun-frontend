    "dev:frontend": "next dev --turbo",
    "test:e2e:dev": "playwright test --config=playwright.config.ts",
    "dev:test:setup": "playwright test --config=playwright.config.ts --project=setup"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "@t3-oss/env-nextjs": "^0.13.8",
