# CalendRun Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# NextAuth Configuration
NEXTAUTH_SECRET=your-nextauth-secret-here
# NEXTAUTH_URL is optional on Vercel - it auto-detects from VERCEL_URL
# NEXTAUTH_URL=http://localhost:3000

# Keycloak Authentication
KEYCLOAK_ISSUER=https://your-keycloak-instance.com/realms/your-realm
KEYCLOAK_CLIENT_ID=your-keycloak-client-id
KEYCLOAK_CLIENT_SECRET=your-keycloak-client-secret

# Flowcore Configuration
# URL format: https://webhook.api.flowcore.io/event/{tenant}/{dataCoreId}/{flowTypeName}/{eventTypeName}?key={apiKey}
FLOWCORE_INGESTION_BASE_URL=https://webhook.api.flowcore.io
FLOWCORE_TENANT=flowcore-saas
FLOWCORE_DATA_CORE_ID=d4e0906a-5ce8-485a-a227-d322f266ae8b
FLOWCORE_API_KEY=your-flowcore-api-key-here

# Backend API URL
NEXT_PUBLIC_BACKEND_API_URL=http://localhost:18765

# Backend API Key (for frontend-backend authentication)
# Must match BACKEND_API_KEY in backend .env file
# Optional: If not set, backend will allow requests without API key (development only)
BACKEND_API_KEY=your-backend-api-key-here

# Optional: App Environment
# NEXT_PUBLIC_APP_ENV=development
